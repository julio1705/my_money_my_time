generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Transitions {
  id             Int          @id @default(autoincrement())
  typeTransition TypeTransition
  value          Float
  dateTime       DateTime      @default(now())
  description    String?       @db.VarChar(150)
}

enum TypeTransition {
  outflow
  inflow
}